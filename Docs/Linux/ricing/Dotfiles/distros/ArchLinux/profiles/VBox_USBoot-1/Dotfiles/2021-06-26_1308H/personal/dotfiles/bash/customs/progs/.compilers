#
# Aliases for Compilers
#

# Libraries to import
libs=(
    ~/personal/codes/compilers.sh
)

# Import Libraries if exist
for lib in "${libs[@]}"; do
    if [ -f $lib ]; then
        . $lib
    fi
done

alias compile_Java="javac"
alias compile_C="gcc"
alias compile_Cpp="cpp_compiler"
alias compile_Cpp_with_wx="cpp_with_wx_compiler"
alias compile_Pyinstaller="pyinstaller"

