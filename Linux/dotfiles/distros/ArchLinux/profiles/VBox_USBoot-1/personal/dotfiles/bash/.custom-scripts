#
# Custom Scripts and Path control
# - Add custom script path to $PATH variable
# 

custom_script_Paths=(
	"~/Desktop/portable/projects/Linux/utilities/dmenu/scripts"
)

for custom_paths in "${custom_script_Paths[@]}"; do
	# echo "Script Path: $custom_paths"
	PATH=$PATH:$custom_paths
done

# echo $PATH
