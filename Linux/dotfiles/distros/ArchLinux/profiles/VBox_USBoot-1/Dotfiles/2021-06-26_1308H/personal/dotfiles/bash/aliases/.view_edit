#
# This Contains aliases for all linux commands
# based on View and Edit
#

# View & Edit
alias view_Bashrc="cat ~/.bashrc"
alias view_personal_Bashrc="cat $paths_dotfiles_bash_bashrc"
alias view_xinitrc="cat ~/.xinitrc"
alias view_xresources="cat ~/.Xresources"
alias view_sxhkdrc="cat ~/.config/sxhkd/sxhkdrc"
alias view_bspwmrc="cat ~/.config/bspwm/bspwmrc"
alias edit_Bashrc="$EDITOR ~/.bashrc"
alias edit_personal_Bashrc="$EDITOR $paths_dotfiles_bash_bashrc"
alias edit_xinitrc="$EDITOR ~/.xinitrc"
alias edit_xresources="$EDITOR ~/.Xresources"
alias edit_sxhkdrc="$EDITOR ~/.config/sxhkd/sxhkdrc"
alias edit_bspwmrc="$EDITOR ~/.config/bspwm/bspwmrc"


